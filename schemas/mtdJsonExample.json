{
    "docker_images": {
        "docker_image1": {
            "docker_image_id": 1,
            "docker_image_description": "test1",
            "tests": [
                "test1",
                "test2"
            ]
        },
        "docker_image2": {
            "docker_image_id": 2,
            "docker_image_description": "test2",
            "tests": [
                "test1",
                "test2",
                "test3"
            ]
        }
    },
    "tests": {
