                }
            ]
        },
        "test2": {
            "test_id": 2,
            "test_blocks": [
                {
                    "block_type": "RUN",
                    "command": "git add .",
                    "command_output_assertion": "VERIFY_EMPTY",
                    "regex": ""
                }
            ]
        },
        "test3": {
            "test_id": 3,
            "test_blocks": [
                {
                    "block_type": "RUN",
                    "command": "git push",
                    "command_output_assertion": "VERIFY_REGEX",
                    "regex": "1234"
                }
            ]
        }
    }
